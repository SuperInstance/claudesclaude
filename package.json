{
  "name": "claudesclaude",
  "version": "0.1.0",
  "description": "Multi-Session Orchestration Plugin for Claude Code",
  "type": "module",
  "private": true,
  "main": "dist/index.js",
  "bin": {
    "claude-multi-session": "dist/cli.js",
    "director": "src/cli.ts"
  },
  "scripts": {
    "start": "node dist/index.js",
    "dev": "node --watch src/cli.ts",
    "build": "tsc",
    "test": "node --test",
    "test:watch": "node --test --watch",
    "test:coverage": "node --test --coverage",
    "lint": "eslint src tests --ext .ts,.tsx",
    "lint:fix": "eslint src tests --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write src tests",
    "format:check": "prettier --check src tests",
    "clean": "rm -rf dist coverage .test-queue .test-registry"
  },
  "dependencies": {
    "chokidar": "^3.5.3",
    "commander": "^11.0.0",
    "express": "^4.18.0",
    "ink": "^3.2.0",
    "simple-git": "^3.19.0",
    "uuid": "^9.0.0",
    "winston": "^3.10.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/express": "^4.17.0",
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.25.0",
    "jest": "^29.0.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.2.0"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0",
    "bun": ">=1.0.0"
  },
  "keywords": [
    "claude",
    "ai",
    "orchestration",
    "multi-agent",
    "development",
    "collaboration",
    "typescript"
  ],
  "author": "SuperInstance",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/SuperInstance/claudesclaude.git"
  },
  "homepage": "https://github.com/SuperInstance/claudesclaude#readme",
  "bugs": {
    "url": "https://github.com/SuperInstance/claudesclaude/issues"
  }
}
