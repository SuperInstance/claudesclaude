{
  "name": "claudesclaude",
  "version": "0.1.0",
  "description": "High-performance multi-agent orchestration system",
  "type": "module",
  "private": true,
  "main": "src/index.ts",
  "scripts": {
    "start": "bun run src/index.ts",
    "dev": "bun --watch src/cli.ts",
    "build": "bun build src/index.ts --outdir dist",
    "test": "bun test",
    "lint": "bunx eslint src tests --ext .ts",
    "type-check": "bunx tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@types/node": "^25.0.8",
    "commander": "^11.1.0",
    "uuid": "^9.0.1",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "typescript": "^5.2.0"
  },
  "engines": {
    "bun": ">=1.0.0"
  },
  "keywords": [
    "orchestration",
    "multi-agent",
    "performance",
    "typescript"
  ],
  "author": "SuperInstance",
  "license": "MIT"
}
