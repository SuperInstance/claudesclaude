{
  "name": "@claudesclaude/orchestration-sdk",
  "version": "1.0.0",
  "description": "Type-safe client SDK for the Claude Orchestration System",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bun run build",
    "dev": "bun run build --watch",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "type-check": "tsc --noEmit",
    "prepublish": "bun run build",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "orchestration",
    "workflow",
    "automation",
    "claudesclaude",
    "typescript",
    "sdk"
  ],
  "author": "Claude Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/claudesclaude/orchestration-sdk"
  },
  "bugs": {
    "url": "https://github.com/claudesclaude/orchestration-sdk/issues"
  },
  "homepage": "https://github.com/claudesclaude/orchestration-sdk#readme",
  "dependencies": {
    "uuid": "^9.0.0",
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/uuid": "^9.0.0",
    "@types/js-yaml": "^4.0.5",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.45.0",
    "typescript": "^5.0.0",
    "bun-types": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "bun": ">=1.0.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "peerDependencies": {
    "bun": ">=1.0.0"
  },
  "optionalDependencies": {
    "ioredis": "^5.3.2",
    "better-sqlite3": "^8.7.0"
  }
}